  "vim.insertModeKeyBindings": [
    {
      "after": ["<esc>", ":", "w", "<CR>"],
      "before": ["<C-s>"]
    },
    {
      "after": ["<esc>", ":", "q", "<CR>"],
      "before": ["<C-q>"]
    }
  ],
  "vim.leader": "<space>",
