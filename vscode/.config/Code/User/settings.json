  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-n>"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "k"],
      "after": ["{"]
    },
    {
      "before": ["<leader>", "j"],
      "after": ["}"]
    },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previousEditor"]
    }
  ],
  "vim.insertModeKeyBindings": [
    // Scape
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["i", "i"],
      "after": ["<Esc>"]
    },
    {
      "before": ["L", "L"],
      "after": ["<Esc>", "l", "a"]
    },
    {
      "before": ["H", "H"],
      "after": ["<Esc>", "h", "a"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q"]
    },
    {
      "before": ["<leader>", "z"],
      "commands": [":q!"]
    },
    // Copy & Paste
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    },
    // Switch divisions
    {
      "before": ["<C-h>"],
      "after": ["<C-w>", "h"]
    },
    {
      "before": ["<C-j>"],
      "after": ["<C-w>", "j"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<C-w>", "k"]
    },
    {
      "before": ["<C-l>"],
      "after": ["<C-w>", "l"]
    },
    // Tabs
    {
      "before": ["<Leader>", "t", "t"],
      "commands": [":tabnew"]
    },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previusEditor"]
    },
    {
      "before": ["<Leader>", "t", "o"],
      "commands": [":tabo"]
    },
    // Search cleaner
    {
      "before": ["<Leader>", "c"],
      "commands": [":noh"]
    },
    // Comment
    {
      "before": ["<Leader>", "/"],
      "commands": ["editor.action.commentLine"]
    },
    // Comands
    {
      "before": ["<Leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<Leader>", "t"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "h"],
      "after": ["_"]
    },
    {
      "before": ["<leader>", "l"],
      "after": ["$"]
    },
    {
      "before": ["<leader>", "i"],
      "after": ["<C-w>", "s"]
    },
    {
      "before": ["<leader>", "s"],
      "after": ["<C-w>", "v"]
    }
  ],

  "vim.visualModeKeyBindings": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    }
  ],

  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<Leader>", "/"],
      "commands": ["editor.action.commentLine"]
    }
  ],

