    "darkGray": "#54524c",
    "darkRed": "#ea6062",
    "darkGreen": "#a9b665",
    "darkYellow": "#d8a657",
    "darkBlue": "#7daea3",
    "darkPurple": "#d3869b",
    "darkAqua": "#89b482",
    "darkOrange": "#fe8019",
    "darkRedBright": "#ea6062",
    "darkGreenBright": "#a9b665",
    "darkYellowBright": "#d8a657",
    "darkBlueBright": "#7daea3",
    "darkPurpleBright": "#d3869b",
    "darkAquaBright": "#89b482",
    "darkOrangeBright": "#fe8019",
    "darkDiffGreen": "#34381b",
    "darkDiffRed": "#402120",
    "lightBg0": "#fbf1c7",
    "lightBg1": "#f4e8be",
    "lightBg2": "#f2e5bc",
    "lightBg3": "#eee0b7",
    "lightFg0": "#654735",
    "lightFg1": "#4f3829",
    "lightGray": "#a89984",
    "lightRed": "#c14a4a",
    "lightGreen": "#6c782e",
    "lightYellow": "#b47109",
    "lightBlue": "#45707a",
    "lightPurple": "#945e80",
    "lightAqua": "#4c7a5d",
    "lightOrange": "#c35e0a",
    "lightDiffGreen": "#e6eabc",
    "lightDiffRed": "#f9e0bb"
